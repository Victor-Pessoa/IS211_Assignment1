class Book:
    author = ""
    title = ""

    def __init__(self, author, title):
        self.author = author
        self.title = title

    def display(self):
        print(self.title, ", written by", self.author)


example1 = Book("J. K. Rowling", "Harry Potter and the Goblet of Fire")
example2 = Book("Walter Scott", "Ivanhoe: A Romance")

example1.display()
example2.display()
